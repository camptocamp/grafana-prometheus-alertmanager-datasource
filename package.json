{
  "name": "camptocamp-prometheus-alertmanager-datasource",
  "version": "2.4.0",
  "description": "Grafana datasource for the prometheus alertmanager",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Camptocamp",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@grafana/e2e-selectors": "^12.3.3",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.0.1",
    "@grafana/plugin-ui": "^0.13.1",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "^1.56.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.15.0",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.14",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.0.3",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "rxjs": "^7.8.2",
    "sass": "^1.93.3",
    "sass-loader": "^16.0.6",
    "semver": "^7.7.3",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.2.0-rc.1",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.2.1",
    "@grafana/i18n": "^12.2.0",
    "@grafana/runtime": "^12.2.1",
    "@grafana/schema": "^12.2.1",
    "@grafana/ui": "^12.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tslib": "^2.8.1"
  },
  "packageManager": "yarn@4.10.3"
}
