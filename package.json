{
  "name": "camptocamp-prometheus-alertmanager-datasource",
  "version": "2.1.0",
  "description": "Grafana datasource for the prometheus alertmanager",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Camptocamp",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^2.0.2",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@swc/core": "^1.11.24",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.38",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.18",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^50.6.14",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.0.2",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.88.0",
    "sass-loader": "16.0.5",
    "semver": "^7.6.3",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "12.0.0",
    "@grafana/runtime": "12.0.0",
    "@grafana/schema": "^11.5.3",
    "@grafana/ui": "12.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tslib": "2.8.1"
  },
  "packageManager": "yarn@4.9.1"
}
